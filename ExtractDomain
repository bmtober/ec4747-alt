#/bin/bash
FILES=./TRAINING/*
for f in $FILES
do
if [[ "${f: -5}" == *.from ]]
then
  sed -i '/@/{s/.*@//}' "$f"
  cat "$f" | tr -cd '[[:alnum:]]._-'
  echo " $f"
fi
done >> CombineList

